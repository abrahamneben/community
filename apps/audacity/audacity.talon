app: audacity
language: en

-

effect amplify: user.menu_select('Effect|Volume and Compression|Amplify...')
rescan audio devices: user.menu_select('Transport|Rescan Audio Devices')
import audio: user.menu_select('File|Import|Audio...')
record track: key(cmd-f10)
stop record: key(cmd-f11)
play track: key(cmd-f8)
stop track: key(cmd-f8)
zoom selection: key(cmd-e)
zoom normal: key(cmd-2)
scan devices: key(cmd-f7)
